{
  "compilerOptions": {
    "target": "es2015",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "@/*":["src/"],
      "Config/*": ["src/config/*"],
      "Assets/*": ["src/assets/*"],
      "Common/*": ["src/common/*"],
      "Components/*": ["src/components/*"],
      "Images/*": ["src/assets/Images"],
      "Utils/*": ["src/service/utils"],
      "Network/*": ["src/service/Network"],
      "Index/*": ["src/pages/index"]
    },
  },
  "exclude": ["node_modules", "dist"],
  "include": ["src"]
}